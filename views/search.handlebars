<form>
    <div>
        <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
        <label class="form-check-label" for="flexCheckDefault">
            Default checkbox
        </label>
    </div>

    <table class="table table-striped">
        {{#each animes as |anime|}}
        <tr>
            <th>Ranking</th>
            <th>{{anime.title}}</th>
            <th>Genre</th>
        </tr>
        {{/each}}
        
    </table>

</form>